{
  "source": "./src",
  "destination": "./esdoc",
  "includes": ["routes", "App.js", "ErrorApp.js", "StatusCode.js", "handleError.js"],
  "plugins": [
    { "name": "esdoc-ecmascript-proposal-plugin", "option": { "all": true } },
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "manual": {
          "globalIndex": true,
          "badge": false,
          "files": ["./docs/type-system.md", "./docs/upgrade-to-v2.md"]
        }
      }
    }
  ]
}
